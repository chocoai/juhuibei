.example--date {
  width:100%;
  text-align:center;
  font-size:10px;
}

.example--date-text {
  display: inline-block;
  margin: 20rpx 0;
  padding: 5rpx 15rpx;
  background: #D4D4D4;
  color: #FFFFFF;
  border-radius: 2px;
}

.example--box {
  overflow: hidden;
  /* border: 1rpx solid #e0e0e0; */
  border-radius: 4px;
  margin-bottom: 30rpx;
  transition: all .2s ease;
  box-shadow: 0 0 1rpx rgba(0, 0, 0, .15); 
  z-index: 10;
  background: #fff;
}

.demo{
  /* padding: 15rpx; */
  /* background: #f6f6f6; */
}

.demo-inner{
  background: #fff;
}

.countdown {
  position: absolute;
  z-index: 100;
}

.label-countdown {
  position: relative;
  left:10rpx;
  top:0rpx;
}

.label-coming {
  position: relative;
  left:20rpx;
  top:0rpx;
}

.center {
  position: absolute;
  text-align: center;
  width: 700rpx;
  z-index: 100;
}

.label-coming-btn {
  display: inline-block;
  margin-top: 70px;
  width: 120px;
  height: 40px;
  border-radius: 20px;
  background: #FF7777;
  color: #FFFFFF;
  line-height: 40px; 
  box-shadow:0 0 4px rgba(0, 0, 0, 0.14), 0 4px 8px rgba(255, 119, 119, 0.28);
}
.dome-image{
  position: relative;
  width: 100%;
  height: 180px; 
}

.meta {
  position: relative;
  /* margin-top: 40rpx;
  border-top: 1rpx solid #e9e9e9; */
  padding: 0rpx 15rpx 15rpx 20rpx;
}

.meta-title {
  position: absolute;
  top: -24rpx;
  line-height: 26rpx;
  padding: 10rpx 14rpx;
  font-size: 26rpx;
  color: #555;
  border-radius: 8rpx;
  background: #f2f2f2;
  transition: background-color .4s ease;
}

.meta-desc {
  /* margin-top: 20rpx; */
  color: #969696;
  font-size: 10px;
}

.meta-icon {
  width: 24rpx;
  height: 24rpx;
  padding: 0 10rpx;
}

.meta-navigator {
  display: inline-block;
}

.navigator-hover {
  color:#ff4444;
}

.example--source {
  position: relative;
}

.view-btn{
  position: absolute;
  right: 20rpx;
  top: -52rpx;
  color: #369;
  opacity: .5;
  font-size: 26rpx;
}

.avatar {
  display:inline-block;
  width:100rpx;
  height:100rpx;
  margin: 10rpx;
}

.button-sp-area{
    margin: 10rpx auto;
    padding-top: 10rpx;
    width: 100%;
    text-align: center;
}

.mini-btn{
    margin-right: 5px;
}

.loear-color {
  color:#ff4444;
}

.dialog-form {

}

.dialog-input {
  border: 1px solid #ebebeb;
  padding:10rpx 40rpx;
  border-radius: 16px;
  margin-bottom: 20rpx;
  height: 22px;
}

.dialog-submit {
  position: relative;
  width: 100%;
  background-color: #FFFFFF;
  color: #FF4444;
  margin-bottom: 20rpx;
}

.new-title {
  color: #000000;
  font-size: 18px;
  line-height: 30px;
  height: 30px;
}

.meta-avatar {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  right: 20rpx;
  top: 8rpx;
  z-index: 10;
  border-radius: 50%;
  overflow: hidden;
}

.meta-image {
  width: 80rpx;
  height: 80rpx;
}