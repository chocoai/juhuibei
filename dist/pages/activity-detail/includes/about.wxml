<view class="music-about">
  <view class="music-content-header">
    <text class="content-label">通讯录</text>
    <view class="content-links" bindtap="switchContent">
      <image class="link" src="/common/assets/images/activity/home_a.png" mode="aspectFill" data-type="story"></image>
      <image class="link" src="/common/assets/images/activity/picture_a.png" mode="aspectFill" data-type="lyric"></image>
      <image class="link" src="/common/assets/images/activity/book_b.png" mode="aspectFill" data-type="about"></image>
    </view>
  </view>
  
  <view class="weui-cells weui-cells_after-title">
    <block wx:for="{{activity_info.info}}" wx:key="{{item}}">
      <block wx:if="{{item.is_coming}}">
        <view class="weui-cell weui-cell_access" hover-class="weui-cell_active" >
            <image src="{{item.user.avatar_url}}" class="call_avatar"></image>
            <view class="weui-cell__bd">
              {{ item.user.username ? item.user.username : item.user.nickname }}
              <block wx:if="{{item.is_master}}">
                <view class="weui-badge">会长</view>
              </block>
            </view>
            <view class="weui-cell__ft weui-cell__ft_in-access" bindtap="callPhone" data-phone="{{item.user.phone}}">
              {{item.user.phone}}
            </view>
        </view>
      </block>
    </block>
  </view>
</view>