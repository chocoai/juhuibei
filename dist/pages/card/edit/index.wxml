
  <view class="header-box"></view>

  <view class="show-box">
    <swiper class="show-swiper"
      indicator-dots="{{indicatorDots}}"
      autoplay="{{autoplay}}"
      interval="{{interval}}"
      duration="{{duration}}"
      vertical="{{vertical}}"
    >
      
        <swiper-item class="show-swiper-item">
          <form bindsubmit="submitForm" report-submit="true">
            <view class="weui-cells weui-cells_after-title">
              <view class="weui-cell weui-cell_input">
                  <view class="weui-cell__bd">
                      <input class="weui-input" placeholder="聚会主题" name="title"/>
                  </view>
              </view>
            </view>
          </form>

        </swiper-item>

    </swiper>
  </view>
  <view class="submit-box" hover-class="submit-btn_active">
      <view class="submit-btn">保存</view>
  </view>
