.my-container {
  width: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 10000;
}

.my-header{
  background-color: #313538;
  color: #fff;
  display: flex;
  align-items: center;
  padding: 30rpx;
  font-size: 28rpx;
  justify-content:center;
  position: relative;
}

.my-avatar {
  display: inline-block;
  width: 200rpx;
  height: 200rpx;
  text-align: center;
}

.my-avatar-img {
  height: 100rpx;
  width: 100rpx;
  border: 2px solid #FFFFFF;
  border-radius: 50%;
}

.my-header {
  background: #FF7777;
}

.my-header text{
  margin-top: 20rpx;
  display: inline-block;
  width: 200rpx;
}

.my-vip {
  position: absolute;
  width: 120rpx;
  height: 40rpx;
  border-radius: 22rpx;
  border:1px solid #FFFFFF;
  bottom: 10rpx;
  left: 10rpx;
  font-size: 10px;
  text-align: center;
  line-height: 40rpx;
  opacity: 0.4;
}

.index-hd {
  
  padding: 70rpx;
  text-align: center;
}
.index-bd {
  padding: 0 30rpx 40rpx;
}
.index-ft {
  padding-bottom: 20rpx;
  text-align: center;
}
.index-logo {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.index-desc {
  margin-top: 20rpx;
  color: #888888;
  font-size: 28rpx;
}


.navigator-box {
  opacity: 0;
  position: relative;
  background-color: #FFFFFF;
  line-height: 1.41176471;
  font-size: 34rpx;

  transform: translateY(-50%);
  transition: .3s;
}
.navigator-box-show {
  opacity: 1;
  transform: translateY(0);
}
.navigator {
  padding: 20rpx 30rpx;
  position: relative;
  display: flex;
  align-items: center;
}
.navigator:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 30rpx;
  height: 1rpx;
  border-top: 1rpx solid #D8D8D8;
  color: #D8D8D8;
}
.navigator:first-child:before {
  display: none;
}

.navigator-day {
  margin-right:20rpx;
  color:#FF7777;
  font-size:10px;
}

.navigator-text {
  flex: 1;
}
.navigator-arrow {
  padding-right: 26rpx;
  position: relative;
}
.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 28rpx;
}


.kind-list-item {
  margin: 20rpx 0;
  background-color: #FFFFFF;
  border-radius: 4rpx;
  overflow: hidden;
}
.kind-list-item:first-child {
  margin-top: 0;
}
.kind-list-text{
  flex: 1;
}
.kind-list-img {
  width: 60rpx;
  height: 60rpx;
}
.kind-list-item-hd {
  padding: 30rpx;
  display: flex;
  align-items: center;

  transition: opacity .3s;
}
.kind-list-item-hd-show {
  color: #FF7777;
}
.kind-list-item-hd-show .kind-list-img {
  opacity: .3;
}
.kind-list-item-bd {
  height: 0;
  overflow: hidden;
}
.kind-list-item-bd-show {
  height: auto;
}


.index-ft {
  margin-top: -25rpx;
}

.github__icon {
  width: 34rpx;
  height: 34rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}
.github__url {
  font-family: Monaco;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  color: #888;
  font-size: 26rpx;
  vertical-align: middle;
}

.menu {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 160rpx;
  text-align: center;
  z-index: 10000;
  background-image: linear-gradient(-180deg,rgba(236,236,234,.00) 0, #FFFFFF 50%);
}

.menu-btn {
  font-size: 12px;
  display: inline-block;
  width: 140rpx;
  height: 60rpx;
  border-radius: 30rpx;
  line-height: 60rpx;
  margin-top: 80rpx;
  color: #FFFFFF;
  background: #FF7777;
  box-shadow:0 0 4px rgba(0, 0, 0, 0.14),0 4px 8px rgba(255, 119, 119, 0.28);
}

.menu-card {
  position: absolute;
  left: 120rpx;
  bottom: 50rpx;
  font-size: 10px;
}

.menu-active {
  width: 150rpx;
  height: 150rpx;
  line-height: 150rpx;
  border-radius: 50%;
  margin-top: 0;
}

.menu-user {
  position: absolute;
  right: 120rpx;
  bottom: 50rpx;
  font-size: 10px;
}