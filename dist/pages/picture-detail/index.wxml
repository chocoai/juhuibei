<wxc-flex class="wrap" cross="top" wrap="wrap">
    <view class="item">
      <view class="item-photo-box">
        <image class="item-photo" src="{{info.img_info.url}}" mode="aspectFill"></image>
        <text class="item-text">{{info.name}}</text>
        <view class="item-desc">
          <text>{{info.description}}</text>
        </view>        
      </view>
    </view> 
</wxc-flex>
<view class="date">
  <view class="user-info">
    <image class="avatar" src="{{info.user_info.avatar_url}}"></image>
    <view class="user-info-text">
      <view><text>{{info.user_info.nickname}}</text></view>
      <view><text>{{info._data}}</text></view>
    </view>
  </view>
</view>
